{
  "border": {
    "width": { "value": "1px", "themeable": true, "source": "$border-width" },
    "color": { "value": "{color.gray.200.value}", "themeable": true, "source": "$border-color" },
    "radius": {
      "base": { "value": ".375rem", "themeable": true, "source": "$border-radius" },
      "lg": { "value": ".425rem", "themeable": true, "source": "$border-radius-lg" },
      "sm": { "value": ".25rem", "themeable": true, "source": "$border-radius-sm" }
    }
  }
}
