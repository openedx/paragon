{
  "color": {
    "$type": "color",
    "progress-bar": {
      "base": {
        "$value": "{color.white}",
        "source": "$progress-bar-color"
      },
      "bg": {
        "base": {
          "$value": "{color.accent.a}",
          "source": "$progress-bar-bg"
        },
        "annotated": {
          "$value": "{color.dark.500}",
          "source": "$annotated-progress-bar-bg"
        }
      },
      "border": {
        "$value": "{color.gray.500}",
        "source": "$progress-bar-border-color"
      }
    },
    "progress": {
      "bg": {
        "$value": "transparent",
        "source": "$progress-bg"
      }
    }
  },
  "elevation": {
    "$type": "shadow",
    "progress-bar": {
      "box-shadow": {
        "$value": "none",
        "source": "$progress-box-shadow"
      }
    }
  }
}
