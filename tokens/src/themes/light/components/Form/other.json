                  "outputReferences": false,
                  "modify": [
                    {
                      "type": "str-replace",
                      "toReplace": "#",
                      "replaceWith": "%23"
                    }
                  ],
                  "source": "$custom-checkbox-indicator-icon-invalid-checked"
