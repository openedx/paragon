{
  "color": {
    "$type": "color",
    "data-table": {
      "bg": {
        "base": {
          "$value": "{color.bg.base}",
          "source": "$data-table-background-color"
        },
        "is-loading": {
          "$value": "{color.white}",
          "modify": [
            {
              "type": "alpha",
              "amount": 0.7
            }
          ],
          "source": "$data-table-is-loading-bg"
        }
      },
      "border": {
        "$value": "{color.light.300}",
        "source": "$data-table-border-color"
      }
    }
  },
  "elevation": {
    "$type": "shadow",
    "data-table": {
      "box-shadow": {
        "$value": "{elevation.box-shadow.sm}",
        "source": "$data-table-box-shadow"
      }
    }
  }
}
