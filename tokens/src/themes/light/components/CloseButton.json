{
  "color": {
    "$type": "color",
    "close-button": {
      "$value": "{color.black}",
      "source": "$close-color"
    }
  },
  "elevation": {
    "$type": "shadow",
    "close-button": {
      "text-shadow": {
        "$value": {
          "color": "{color.white}",
          "offsetX": "0",
          "offsetY": "1px",
          "blur": "0"
        },
        "source": "$close-text-shadow"
      }
    }
  }
}
