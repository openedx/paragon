{
  "color": {
    "$type": "color",
    "code": {
      "base": {
        "$value": "#E83E8C",
        "source": "$code-color"
      },
      "kbd": {
        "base": {
          "$value": "{color.white}",
          "source": "$kbd-color"
        },
        "bg": {
          "$value": "{color.gray.700}",
          "source": "$kbd-bg"
        }
      },
      "pre": {
        "$value": "{color.gray.900}",
        "source": "$pre-color"
      }
    }
  },
  "elevation": {
    "$type": "shadow",
    "code": {
      "kbd": {
        "box-shadow": {
          "$value": "none",
          "source": "$kbd-box-shadow"
        }
      }
    }
  }
}
