{
  "typography": {
    "headings": {
      "font": {
        "family": {
          "$type": "fontFamily",
          "$value": "inherit",
          "source": "$headings-font-family"
        },
        "weight": {
          "$type": "fontWeight",
          "$value": "{typography.font.weight.bold}",
          "source": "$headings-font-weight"
        }
      },
      "line-height": {
        "$type": "number",
        "$value": "1.25",
        "source": "$headings-line-height"
      }
    }
  },
  "spacing": {
    "$type": "dimension",
    "headings": {
      "margin-bottom": {
        "$value": ".5rem",
        "source": "$headings-margin-bottom"
      }
    }
  }
}
