{
  "size": {
    "$type": "dimension",
    "caret": {
      "width": {
        "$value": ".3em",
        "source": "$caret-width"
      }
    }
  },
  "spacing": {
    "$type": "dimension",
    "caret": {
      "base": {
        "$value": ".255em",
        "source": "$caret-spacing"
      },
      "vertical-align": {
        "$value": ".255em",
        "source": "$caret-vertical-align"
      }
    }
  }
}
