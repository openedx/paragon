{
  "typography": {
    "$type": "percentage",
    "image": {
      "figure-caption": {
        "font-size": {
          "$value": "90%",
          "source": "$figure-caption-font-size"
        }
      }
    }
  },
  "size": {
    "$type": "dimension",
    "image": {
      "thumbnail": {
        "border": {
          "width": {
            "$value": "{size.border.width}",
            "source": "$thumbnail-border-width"
          },
          "radius": {
            "$value": "{size.border.radius.base}",
            "source": "$thumbnail-border-radius"
          }
        }
      }
    }
  },
  "spacing": {
    "$type": "dimension",
    "image": {
      "thumbnail": {
        "padding": {
          "$value": ".25rem",
          "source": "$thumbnail-padding"
        }
      }
    }
  }
}
