{
  "spacing": {
    "$type": "dimension",
    "breadcrumb": {
      "margin": {
        "left": {
          "$value": ".5rem",
          "source": "$breadcrumb-margin-left"
        }
      }
    }
  },
  "size": {
    "$type": "dimension",
    "breadcrumb": {
      "border": {
        "radius": {
          "focus": {
            "$value": ".125rem",
            "source": "$breadcrumb-focus-border-radius"
          }
        },
        "axis": {
          "x-focus": {
            "$value": ".25rem",
            "source": "$breadcrumb-border-focus-axis-x"
          },
          "y-focus": {
            "$value": ".5rem",
            "source": "$breadcrumb-border-focus-axis-y"
          }
        },
        "width-focus": {
          "$value": ".0625rem",
          "source": "$breadcrumb-border-focus-width"
        }
      }
    }
  }
}
