{
  "size": {
    "$type": "dimension",
    "icon-button": {
      "diameter": {
        "md": {
          "$value": "2.75rem",
          "source": "$btn-icon-diameter-md"
        },
        "sm": {
          "$value": "2.25rem",
          "source": "$btn-icon-diameter-sm"
        },
        "inline": {
          "$value": "calc({typography.line-height.base} * 1em + .1em)",
          "source": "$btn-icon-diameter-inline"
        }
      }
    }
  }
}
