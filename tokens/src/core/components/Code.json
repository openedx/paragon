{
  "typography": {
    "code": {
      "font-size": {
        "source": "$code-font-size",
        "$value": "87.5%",
        "$type": "percentage"
      },
      "kbd": {
        "font-size": {
          "source": "$kbd-font-size",
          "$value": "{typography.code.font-size}",
          "$type": "dimension"
        },
        "nested": {
          "$type": "fontWeight",
          "font-weight": {
            "source": "$nested-kbd-font-weight",
            "$value": "{typography.font.weight.bold}"
          }
        }
      }
    }
  },
  "spacing": {
    "$type": "dimension",
    "code": {
      "kbd": {
        "padding": {
          "y": {
            "source": "$kbd-padding-y",
            "$value": ".2rem"
          },
          "x": {
            "source": "$kbd-padding-x",
            "$value": ".4rem"
          }
        }
      }
    }
  },
  "size": {
    "$type": "dimension",
    "code": {
      "pre": {
        "scrollable": {
          "max-height": {
            "source": "$pre-scrollable-max-height",
            "$value": "340px"
          }
        }
      }
    }
  }
}
