{
  "size": {
    "$type": "dimension",
    "tabs": {
      "notification": {
        "height": {
          "$value": "1rem",
          "source": "$tab-notification-height"
        },
        "width": {
          "$value": "1rem",
          "source": "$tab-notification-width"
        }
      }
    }
  },
  "typography": {
    "$type": "dimension",
    "tabs": {
      "notification": {
        "font-size": {
          "$value": "{typography.font.size.xs}",
          "source": "$tab-notification-font-size"
        }
      }
    }
  }
}
