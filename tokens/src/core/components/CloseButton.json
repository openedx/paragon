{
  "typography": {
    "close-button": {
      "font": {
        "size": {
          "source": "$close-font-size",
          "$value": "calc({typography.font.size.base} * 1.5)",
          "$type": "dimension"
        },
        "weight": {
          "source": "$close-font-weight",
          "$value": "{typography.font.weight.bold}",
          "$type": "fontWeight"
        }
      }
    }
  }
}
