{
  "selectable-box": {
    "padding": { "value": "1rem", "themeable": true, "source": "$selectable-box-padding" },
    "border": {
      "radius": { "value": ".25rem", "themeable": true, "source": "$selectable-box-border-radius" }
    },
    "box": {
      "space": { "value": ".75rem", "themeable": true, "source": "$selectable-box-space" }
    },
    "cols": {
      "number": {
        "min": { "value": "1", "themeable": true, "source": "$min-cols-number" },
        "max": { "value": "12", "themeable": true, "source": "$max-cols-number" }
      }
    }
  }
}
