{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "test": {
      "plugins": ["@babel/plugin-proposal-class-properties"],
      "presets": [
        [
          "@babel/preset-env"
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
      ]
    }
  }
}
