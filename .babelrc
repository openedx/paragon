{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      },
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "test": {
      "plugins": ["require-context-hook", "@babel/plugin-proposal-class-properties"],
      "presets": [
        [
          "@babel/preset-env",
        ],
        "@babel/preset-react"
      ],
    }
  }
}
