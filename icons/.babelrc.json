{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@svgr/babel-plugin-add-jsx-attribute",
      {
        "elements": ["path"],
        "attributes": [
          {
            "name": "fill",
            "value": "currentColor",
            "spread": false,
            "literal": false,
            "position": "start"
          }
        ]
      }
    ]
  ]
}
